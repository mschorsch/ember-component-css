{
  "name": "ember-component-css",
  "version": "1.0.0-alpha.1",
  "description": "An Ember CLI addon which allows you to specify styles for individual components",
  "keywords": [
    "ember-addon"
  ],
  "repository": "github:ebryn/ember-component-css",
  "license": "MIT",
  "author": "Erik Bryn",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "standard-version",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext js,ts .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "broccoli-file-manifest": "^2.0.2",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^2.3.1",
    "broccoli-stew": "^3.0.0",
    "broccoli-tree-walker": "^1.0.0",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-typescript": "^3.1.3",
    "fs-extra": "^8.1.0",
    "md5": "^2.2.1",
    "postcss": "^7.0.17",
    "postcss-less": "^3.1.4",
    "postcss-scss": "^2.0.0",
    "postcss-selector-namespace": "^3.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@types/console-ui": "^2.2.3",
    "@types/core-object": "^3.0.1",
    "@types/ember": "^3.1.2",
    "@types/ember-qunit": "^3.4.8",
    "@types/ember__test-helpers": "^0.7.10",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^12.12.34",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-plugin": "^4.0.2",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.17.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-styles-preprocessor": "^0.5.5",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-node": "^11.0.0",
    "fs-tree-diff": "^2.0.1",
    "loader.js": "^4.7.0",
    "no-style-files-yet": "link:./tests/dummy/lib/no-style-files-yet",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0",
    "second-test-addon": "link:./tests/dummy/lib/second-test-addon",
    "standard-version": "^7.0.0",
    "test-addon": "link:./tests/dummy/lib/test-addon",
    "ts-node": "^8.8.2",
    "typescript": "~3.7.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "before": [
      "ember-cli-babel",
      "ember-cli-styles-preprocessor",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "configPath": "tests/dummy/config"
  }
}
